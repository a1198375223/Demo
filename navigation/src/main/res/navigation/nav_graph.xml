<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navHostFragment">


    <fragment
        android:id="@+id/navHostFragment"
        android:name="com.example.navigation.NavHostFragment"
        android:label="fragment_nav_host"
        tools:layout="@layout/fragment_nav_host" />
    <fragment
        android:id="@+id/flowStepFragment"
        android:name="com.example.navigation.FlowStepFragment"
        android:label="flow_step_one_dest"
        tools:layout="@layout/flow_step_one_dest" >
        <action
            android:id="@+id/action_flowStepFragment_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.navigation.BlankFragment"
        android:label="flow_step_two_fragment"
        tools:layout="@layout/flow_step_two_fragment" >
        <action
            android:id="@+id/action_blankFragment_to_navHostFragment"
            app:destination="@id/navHostFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.example.navigation.BlankFragment2"
        android:label="deeplink_dest"
        tools:layout="@layout/deeplink_dest" />
</navigation>